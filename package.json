{
  "name": "bare-amd-loader",
  "version": "0.1.2",
  "description": "Simple AMD loader dedicated to bare module specifier support.",
  "keywords": [
    "amd",
    "loader",
    "module"
  ],
  "license": "MIT",
  "repository": "github:miguel-silva/bare-amd-loader",
  "bugs": "https://github.com/miguel-silva/bare-amd-loader/issues",
  "main": "dist/index.js",
  "module": "dist/bare-amd-loader.esm.js",
  "unpkg": "dist/bare-amd-loader.umd.production.min.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build --format cjs,esm,umd",
    "test": "tsdx test",
    "lint": "tsdx lint"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "husky": "^3.0.4",
    "nanoid": "^3.1.31",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "tsdx": "^0.9.0",
    "tslib": "^1.10.0",
    "typescript": "^3.5.3"
  }
}
